{
    "trailingComma": "es5",
    "tabWidth": 4,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "@/views/(.*)$",
        "@/components/(.*)$",
        "@/context/(.*)$",
        "@/reducers/(.*)$",
        "@/hooks/(.*)$",
        "@/styles/(.*)$",
        "@/utils/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
