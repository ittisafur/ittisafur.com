{
    "trailingComma": "es5",
    "tabWidth": 4,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "bracketSameLine": true,
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "^views/(.*)$",
        "^components/(.*)$",
        "^context/(.*)$",
        "^reducers/(.*)$",
        "^hooks/(.*)$",
        "^styles/(.*)$",
        "^utils/(.*)$",
        "^marketing/(.*)$",
        "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}
